TUTTI I PRODOTTI DESTINATI IN EUROPA 
SELECT * FROM PRODOTTO
WHERE DESTINAZIONE = 'EUROPA';

 TUTTI I PRODOTTI CHE HANNO UN PREZZO MAGGIORE DI 188
 SELECT * FROM PRODOTTO
 WHERE PREZZO > 188 ;

 TUTTI COLORO CHE HANNO SELEZIONATO COME METODO DI PAGAMENTO LA CARTA DI CREDITO 
 SELECT * FROM ACQUIRENTE 
 WHERE METODO_PAGAMENTO = 'CARTA DI CREDITO';

 TUTTI COLORO CHE NON HANNO PAGATO CON LA CARTA DI CREDITO 
 SELECT * FROM ACQUIRENTE 
 WHERE NOT METODO_PAGAMENTO = 'CARTA DI CREDITO';

 IL PREZZO DEI PRODOTTI ORDINATO IN MANIERA CRESCENTE 
 SELECT *  FROM PRODOTTO 
 ORDER BY PREZZO ASC;

  TUTTI I PRODOTTI ORDINATI COL PREZZO IN MANIERA DECRESCENTE
  SELECT * FROM PRODOTTO 
  ORDER BY PREZZO DESC;